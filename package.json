    "@layerzerolabs/oft-evm": "^3.0.0",
    "@rainbow-me/rainbowkit": "^2.1.4",
    "@tanstack/react-query": "^5.49.2",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "@vercel/node": "^3.2.8",
    "axios": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "react-router": "^6.24.0",
    "react-router-dom": "6",
    "rxjs": "^7.8.1",
    "ts-node-dev": "^2.0.0",
