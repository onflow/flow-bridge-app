{
  "name": "flow-bridge-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsc -p tsconfig.api.json --watch\" \"nodemon dist/api/proxy.js\"",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "node dist/api/proxy.js",
    "build:server": "tsc -p tsconfig.api.json",
    "vercel-build": "npm run build && npm run build:server"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@axelar-network/axelarjs-sdk": "^0.16.2",
    "@axelar-network/axelarjs-types": "^0.33.0",
    "@rainbow-me/rainbowkit": "^2.1.3",
    "@tanstack/react-query": "^5.49.2",
    "@types/axios": "^0.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.10",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^6.24.0",
    "react-router-dom": "^6.24.0",
    "rxjs": "^7.8.1",
    "unfetch": "^5.0.0",
    "viem": "^2.18.2",
    "wagmi": "^2.12.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3",
    "vite": "^5.3.1",
    "concurrently": "^6.0.0",
    "nodemon": "^2.0.7"
  }
