{
    "version": 2,
    "builds": [
      {
        "src": "src/api/proxy.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["build/**"]
        }
      },
      {
        "src": "vite.config.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/api/proxy.ts"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }